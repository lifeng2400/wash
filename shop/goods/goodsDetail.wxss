}
.choose .chooseView button.btn1{
  background-color: #F44225;
  color:#fff;
}
/* .choose .chooseView button.btn1::after{
  border:none;
} */
.choose .chooseView button.btn2{
  background-color: #fff;
  color:#282828;
}
.chooseView:nth-child(3) .chooseViewL{
  display: flex;
  align-items: center
}
.chooseViewL,.chooseViewLR{
 flex:1;
 text-align: right;
 margin-right: 0 !important;
}

/* 加减 */
/*主容器*/
.stepper {
	width:136rpx;
	height:42rpx;
	/*给主容器设一个边框*/
	border: 2rpx solid #c5c5c5;
	border-radius: 18rpx;
}
 
/*加号和减号*/
.stepper text {
	width:40rpx;
	line-height: 42rpx;
	text-align: center;
	float: left;
}
 
/*数值*/
.stepper input {
	width:50rpx;
	height: 42rpx !important;
  min-height: 42rpx !important;
	float: left;
	margin: 0 auto;
	text-align: center;
	font-size: 24rpx;
	/*给中间的input设置左右边框即可*/
	border-left: 2rpx solid #c5c5c5;
	border-right: 2rpx solid #c5c5c5;
}
 
/*普通样式*/
.stepper .normal{
	color: black;
}
 
/*禁用样式*/
.stepper .disabled{
	color: #c5c5c5;
}

