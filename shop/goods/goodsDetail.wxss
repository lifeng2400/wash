/* 商品详情 */
.details{
  height:230rpx;
  background-color: #fff;
  margin-bottom: 12rpx;
}
.details .detailsTop,.details .detailsMid,.details .detailsBottom{
  display: flex;
  justify-items: center;
}
.details .detailsMid{
  padding-left:24rpx;
}
.details .detailsTop,.details .detailsBottom{
 padding:14rpx 24rpx;
}
.details .detailsTop .price{
  flex:1
}
.details .detailsTop .price text:nth-child(1){
   font-size:36rpx;
   color:#F44225;
   font-weight:500
}
.details .detailsTop .price text:nth-child(2){
  font-size: 24rpx;
  color:#C5C5C5;
  text-decoration:line-through;
}
.details .detailsTop .num{
  flex:1;
  text-align: right;
  font-size: 22rpx;
  color:#C5C5C5;
}
.details image{
  width:22rpx;
  height:24rpx;
  margin:0 20rpx;
}
.details .share{
  width:120rpx;
  height:56rpx;
  margin-left:30rpx;
  display: flex;
  justify-items: center;
  align-items: center;
  background-color: #EFEFF4;
  font-size: 22rpx;
  color:#7B7B7B;
  border-bottom-left-radius: 28rpx;
  border-top-left-radius: 28rpx;
}
.details .detailsMid view:nth-child(1){
  color:#282828;
  font-size: 26rpx;
  flex: 1;
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.details .detailsBottom{
  font-size: 22rpx;
  color:#C5C5C5;
}

/* 选择规格 */
.choose{
  background-color: #fff;
  margin-bottom:24rpx;
  font-size: 24rpx;
  padding:0 24rpx;
}
.choose .chooseView{
